{
  "CosmosDB": {
    "Endpoint": "[ENTER_YOUR_ENDPOINT_HERE]",
    "AuthKey": "<COSMOS_DB_AUTHKEY>",
    "DatabaseId": "platform",
    "ContainerPrefix": "USER",
    "CreateIfNotExists": true,
    "DatabaseRUs": 400
  },
  "Authentication": {
    "JWTSelfIssuer": "[ENTER_COMPANY_NAME_HERE]",
    "JWTSelfIssuedAudience": "your-domain.com",
    "JWTP12CertificateLocationOld": "AuthCertificate/prd-old.p12",
    "JWTCertificatePasswordOld": "<JWT_OLD_PASSWORD>",
    "JWTP12CertificateLocation": "AuthCertificate/prd.p12",
    "JWTCertificatePassword": "<JWT_PASSWORD>",
    "AADTenantId": "[ENTER_YOUR_PRD_AAD_TENTANT_ID_HERE]",
    "AADServicePrincipalId": "[ENTER_YOUR_PRD_AAD_SERVICE_PRINCIPAL_ID_HERE]",
    "AADServicePrincipalPassword": "<AAD_SERVICE_PRINCIPAL_PASSWORD>",
    "AADGroupsAsRoles": [
      {
        "RoleName": "sysAdmin",
        "GroupObjectId": "[ENTER_YOUR_PRD_SYS_ADMIN_GROUP_ID_HERE]"
      },
      {
        "RoleName": "generalAdmin",
        "GroupObjectId": "[ENTER_YOUR_PRD_GEN_ADMIN_GROUP_ID_HERE]"
      }
    ]
  }
}